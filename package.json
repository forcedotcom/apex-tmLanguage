{
  "name": "apex-tmlanguage",
  "version": "0.1.0",
  "description":
    "Textmate grammar for Apex with outputs for VSCode, Atom and TextMate.",
  "displayName": "apex-tmLanguage",
  "keywords": ["salesforce-dx", "salesforce", "apex"],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/forcedotcom/apex-tmLanguage/issues"
  },
  "contributors": [
    {
      "name": "Nathan Totten"
    }
  ],
  "scripts": {
    "compile": "node ./node_modules/typescript/bin/tsc -p .",
    "watch": "node ./node_modules/typescript/bin/tsc -w -p .",
    "test": "gulp test",
    "prepublish": "gulp",
    "build:default": "gulp default",
    "format":
      "prettier --config .prettierrc.json --write './**/*.{ts,js,json,md}'"
  },
  "dependencies": {},
  "devDependencies": {
    "@types/chai": "^3.4.34",
    "@types/gulp": "^4.0.3",
    "@types/mocha": "^2.2.33",
    "chai": "^3.5.0",
    "gulp": "^3.9.1",
    "gulp-json2cson": "^1.0.1",
    "gulp-mocha": "^3.0.1",
    "gulp-typescript": "^3.2.2",
    "gulp-yaml": "^1.0.1",
    "js-yaml": "^3.7.0",
    "mocha": "^3.2.0",
    "plist": "^2.0.1",
    "prettier": "^1.12.1",
    "typescript": "^2.5.2",
    "vscode-textmate": "^2.3.2"
  }
}
